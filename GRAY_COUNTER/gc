    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x55d8cff64d60_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x55d8cff64d60_0;
    %parti/s 1, 0, 2;
    %xor;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x55d8cff64e40_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55d8cff51910;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d8cff65040_0, 0, 1;
T_1.0 ;
