T_0.0 ;
    %load/vec4 v0x55d8cff64d60_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55d8cff64d60_0, 0;
    %load/vec4 v0x55d8cff64d60_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x55d8cff64d60_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x55d8cff64d60_0;
    %parti/s 1, 2, 3;
    %xor;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x55d8cff64d60_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x55d8cff64d60_0;
    %parti/s 1, 1, 2;
