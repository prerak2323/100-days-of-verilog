T_0.0 ;
    %load/vec4 v0x55ef7dfc0210_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %assign/vec4 v0x55ef7dfc0210_0, 0;
    %load/vec4 v0x55ef7dfc0210_0;
    %parti/s 1, 7, 4;
    %ix/load 5, 0, 0;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x55ef7dfc0210_0, 4, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55ef7dfae860;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ef7dfc0410_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55ef7dfae860;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55ef7dfc0410_0;
    %inv;
    %store/vec4 v0x55ef7dfc0410_0, 0, 1;
    %jmp T_2;
