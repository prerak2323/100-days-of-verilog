 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 32 "count";
v0x5619ec364860_0 .net "clk", 0 0, v0x5619ec3888a0_0;  1 drivers
v0x5619ec3886a0_0 .var "count", 31 0;
v0x5619ec388780_0 .net "rst", 0 0, v0x5619ec388a40_0;  1 drivers
E_0x5619ec3784c0 .event posedge, v0x5619ec388780_0, v0x5619ec364860_0;
    .scope S_0x5619ec33dfa0;
T_0 ;
    %wait E_0x5619ec3784c0;
    %load/vec4 v0x5619ec388780_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5619ec3886a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5619ec3886a0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x5619ec3886a0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5619ec33de10;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5619ec3888a0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5619ec33de10;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5619ec3888a0_0;
    %inv;
    %store/vec4 v0x5619ec3888a0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5619ec33de10;
T_3 ;
    %vpi_call 2 19 "$dumpfile", "counter_result.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5619ec33de10 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5619ec388a40_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5619ec388a40_0, 0, 1;
    %delay 500, 0;
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_3;
